generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Usuarios {
  id         Int      @id @default(autoincrement())
  nombre     String
  apellido   String
  correo     String   @unique
  cedula     String
  nacimiento DateTime @db.Date
  sexo       String
  // tipo       Tipo     @default(PERSONAL)
  // sucursal   String
}

